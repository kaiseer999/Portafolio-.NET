@model HomeIndexDTO

@{
    ViewData["Title"] = "Cristian Mejia - Desarrollador Junior";
}



<partial name="_fotoIntro"/>


<partial name="_redesSociales" />


<partial name="_sobreMi" />

<partial name="_experiencia"/>


<partial name="_proyectos" model="Model.Proyectos" />


<partial name="_formularioContacto" />

@section Scripts {
    <script>
        $(document).ready(function () {
        @if (TempData["SuccessMessage"] != null)
        {
            <text>
                    Swal.fire({
                        icon: 'success',
                        title: 'Éxito',
                        text: '@TempData["SuccessMessage"]'
                    });
            </text>
        }

        @if (TempData["ErrorMessage"] != null)
        {
            <text>
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: '@TempData["ErrorMessage"]'
                    });
            </text>
        }
                        });
    </script>
}
